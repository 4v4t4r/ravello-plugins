<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
		<parent>
		<groupId>com.ravello.maven.demo</groupId>
		<artifactId>ravello-maven-demo</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.ravello.demo.application</groupId>
	<artifactId>ravello-maven-demo-application</artifactId>
	<version>1.0</version>
	<name>ravello-maven-demo-application</name>
	<packaging>pom</packaging>
	<build>
		<plugins>
			<plugin>
				<groupId>com.ravello.plugins</groupId>
				<artifactId>ravello-maven-plugin</artifactId>
				<version>0.0.1-SNAPSHOT</version>
				<executions>
					<execution>
						<phase>deploy</phase>
						<goals>
							<goal>${qoal}</goal>
						</goals>
						<configuration>
							<userName>${user}</userName>
							<password>${password}</password>
							<blueprintName>${bp-name}</blueprintName>
							<applicationName>${app-name}</applicationName>
							<preferredCloud>${cloud}</preferredCloud>
							<preferredZone>${zone}</preferredZone>
							<finalName>${final-name}</finalName>
							<classifier>${app-classifier}</classifier>
							<timeout>${timeout}</timeout>
							<delay>${delay}</delay>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<pluginRepositories>
		<pluginRepository>
			<id>plugins-repos</id>
			<name>management-svn-releases</name>
			<url>http://10.0.2.67:8081/artifactory/plugins-release</url>
		</pluginRepository>
		<pluginRepository>
			<id>plugins-repos-snapshot</id>
			<name>management-svn-snapshots</name>
			<url>http://10.0.2.67:8081/artifactory/plugins-snapshot</url>
		</pluginRepository>
	</pluginRepositories>
	<distributionManagement>
		<repository>
			<id>management-svn</id>
			<name>management-svn-releases</name>
			<url>http://10.0.2.67:8081/artifactory/libs-release-local</url>
		</repository>
	</distributionManagement>
</project>