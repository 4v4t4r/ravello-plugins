<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.ravello.maven.demo</groupId>
	<artifactId>ravello-maven-demo</artifactId>
	<version>1.0</version>
	<packaging>pom</packaging>
	<modules>
		<module>ravello-maven-demo-webapp</module>
		<module>ravello-maven-demo-application</module>
		<module>ravello-maven-demo-deploy</module>
	</modules>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.0</version>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<pluginRepositories>
		<pluginRepository>
			<id>plugins-repos</id>
			<name>management-svn-releases</name>
			<url>http://10.0.2.67:8081/artifactory/plugins-release-local</url>
		</pluginRepository>
		<pluginRepository>
			<id>management-svn</id>
			<name>management-svn-releases</name>
			<url>http://10.0.2.67:8081/artifactory/libs-release-local</url>
		</pluginRepository>
	</pluginRepositories>
	<repositories>
		<repository>
			<id>management-svn</id>
			<name>management-svn-releases</name>
			<url>http://10.0.2.67:8081/artifactory/libs-snapshot-local</url>
		</repository>
		<repository>
			<id>management-svn</id>
			<name>management-svn-releases</name>
			<url>http://10.0.2.67:8081/artifactory/libs-release-local</url>
		</repository>
	</repositories>
	<distributionManagement>
		<repository>
			<id>management-svn</id>
			<name>management-svn-releases</name>
			<url>http://10.0.2.67:8081/artifactory/libs-release-local</url>
		</repository>
	</distributionManagement>

</project>